{
    "appname": "web_channel_spring",
    "namespace": "shank",
    "alias": "spring",
    "debug": false,
    "port": 3000,
    "outputStd": false,
    "viewsdir": "dist",
    "apiPrefix": "",
    "cacheURL": true,
    "statistics": true,
    "statisticsJaeger": true,
    "statisticsProm": true,
    "JaegerDebug": false,
    "JaegerSamplerType": "probabilistic",
    "JaegerSamplerParam": 0.1,
    "logs": [
        {
            "type": "rotating-file",
            "path": "/data/logs/out.log",
            "level": "info",
            "period": "1d",
            "count": 30
        },
        {
            "type": "rotating-file",
            "path": "/data/logs/err.log",
            "level": "error",
            "period": "1d",
            "count": 30
        }
    ],
    "redisHosts": [],
    "blacklist": ["/controllers/black/black.js"],
    "cdnCacheTime": 120
}
