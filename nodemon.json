{
  "restartable": "rs",
  "ignore": [".git", "node_modules","logs","tests","views","client","devtmp","package.json","webpack.config.js"],
  "verbose": false,
  "execMap": {

  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "biz",
    "app.js"
  ],
  "env": {
    "NODE_ENV": "development",
    "PORT": "3000"
  },
  "ext": "js json",
  "legacy-watch": false
}