{
  "restartable": "rs",
  "ignore": [".git", "node_modules","logs","tests","views","client"],
  "verbose": true,
  "execMap": {

  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "biz"
  ],
  "env": {
    "NODE_ENV": "development",
    "PORT": "3000"
  },
  "ext": "js json",
  "legacy-watch": false
}