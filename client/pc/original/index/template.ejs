<!DOCTYPE html>
<html xmlns:wb="http://open.weibo.com/wb">

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="always" name="referrer">
    <title>独家-凤凰网</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    {{{ htmlWebpackPlugin.options.errorupload }}}
    <script>
        var allData = <%- JSON.stringify(allData).replace(/</g, '\\u003c') %>;
    </script>
    {{#each htmlWebpackPlugin.files.css}}
    <link href="{{path}}" rel="stylesheet"> 
    {{/each}}

    {{{ htmlWebpackPlugin.options.polyfill }}}
</head>

<body>
    <div id="root"></div>

    {{#if htmlWebpackPlugin.options.useMorden}}
    {{#if htmlWebpackPlugin.options.isDevMode}}
        {{#each htmlWebpackPlugin.files.js}}
            <script type="module" src="{{path}}_modern.js" type="text/javascript" crossorigin></script>
        {{/each}}
    {{else}}
        <!--include file="{{htmlWebpackPlugin.options.includeName}}.html" -->
    {{/if}}
    {{{htmlWebpackPlugin.options.modernPolyfill}}}
    {{#each htmlWebpackPlugin.files.js}}
        <script nomodule src="{{path}}" type="text/javascript" crossorigin></script>
    {{/each}}
    {{else}}
        {{#each htmlWebpackPlugin.files.js}}
            <script src="{{path}}" type="text/javascript" crossorigin></script>
        {{/each}}
    {{/if}}
</body>

</html>