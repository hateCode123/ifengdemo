<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="always" name="referrer">
    <meta name=viewport content="width=device-width,initial-scale=1,user-scalable=no,viewport-fit=cover">
    <title>mobile</title>
    {{{ htmlWebpackPlugin.options.errorupload }}}
    <script>
        var __jsonData__ = <%- JSON.stringify(allData) %>;
    </script>
    {{#each htmlWebpackPlugin.files.css}}
    <link href="{{path}}" rel="stylesheet"> 
    {{/each}}
</head>

<body>
    <div id="root"></div>
    {{#if htmlWebpackPlugin.options.useMorden}}
        {{#if htmlWebpackPlugin.options.isDevMode}}
            {{#each htmlWebpackPlugin.files.js}}
                <script type="module" src="{{path}}_modern.js" type="text/javascript"></script>
            {{/each}}
        {{else}}
            <!--include file="{{htmlWebpackPlugin.options.includeName}}.html" -->
        {{/if}}
        {{{htmlWebpackPlugin.options.modernPolyfill}}}
        {{#each htmlWebpackPlugin.files.js}}
            <script nomodule src="{{path}}" type="text/javascript"></script>
        {{/each}}
    {{else}}
        {{#each htmlWebpackPlugin.files.js}}
            <script src="{{path}}" type="text/javascript"></script>
        {{/each}}
    {{/if}}
</body>

</html>
